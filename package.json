{
  "name": "playwright",
  "config": {
    "url": "https://newspress.dev.dowjones.io/mg/wp-admin/"
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "example": "playwright test .pom/tests --project='firefox' --headed",
    "saveStorage": "playwright codegen $npm_package_config_url --save-storage='dev.json' --browser='firefox'",
    "exploreNS": "playwright test .pom/tests --load-storage='dev.json' --browser='firefox'",
    "NS": "URL=$npm_package_config_url npx playwright test .pom/tests --grep @NS --project='firefox' --repeat-each 1 --workers 5",
    "NSH": "URL=$npm_package_config_url npx playwright test .pom/tests --grep @NS --headed --project='firefox' --repeat-each 1 --workers 2",
    "NSST": "URL=$npm_package_config_url npx playwright test .pom/tests --grep @smokeTest --headed --project='firefox' --repeat-each 1 --workers 5",
    "PT": "URL=$npm_package_config_url npx playwright test .pom/tests --grep @PT --project='firefox' --repeat-each 1 --workers 3",
    "NSD": "URL=$npm_package_config_url npx playwright test .pom/tests --grep @NS --headed --project='firefox' --debug"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.32.2"
  },
  "dependencies": {
    "otplib": "^12.0.1",
    "playwright": "^1.32.1"
  }
}
